package selfProgramming;

public class Integer {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int number1 = 12;
		int number2 = 24;
		int number3 = 36;
		int sum;
		sum = number1 + number2 + number3;
		
		System.out.println("The sum of 3 number is " + sum);

	}

}
